{
    "name": "Deal Finder",
    "description": "Find best deals on google",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "content_scripts": [{
        "matches": [
            "https://www.amazon.in/*"
        ],
        "js": [
            "popup.js"
        ]
    }]
}