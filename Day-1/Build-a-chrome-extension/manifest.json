{
    "name": "AutoLHD",
    "version": "1.0",
    "manifest_version": 2,
    "icons" : {
	"16" : "original.png",
	"32" : "original.png",
	"64" : "original.png"
    },
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": [
            "jquery.js",
            "script.js"
        ]
    },
    "content_scripts": [{
        "matches": [
            "https://localhackday.mlh.io/schedule?reason=AutoLHD"
        ],
        "js": ["jquery.js", "scheduler.js"]
    }]
}
